<UserControl x:Class="Naficebi.UserControll.LeftPanelWithoutItems"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Naficebi.UserControll"
             xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
             mc:Ignorable="d" 
             x:Name="leftPanel"
             d:DesignHeight="450" d:DesignWidth="800">
    
    
    <Border   >
        <Grid>
            <StackPanel x:Name="ButtonsPanel">
                <local:ProcessPageBaner Margin="0 0 0 10" x:Name="Banner" />
                <local:ProcessLeftButton Kind="Database" CaseNumber="მონაცემთა ბაზის არჩევა"  x:Name="openDbAccess"/>
                <local:ProcessLeftButton Kind="CodeArray"  CaseNumber="300 კანდინანტის შერჩევა"/>
                <local:ProcessLeftButton Kind="CodeArray"  CaseNumber="დამატებით 300 კანდინანტის შერჩევა"/>
            </StackPanel>
            
        </Grid>
    </Border>
</UserControl>
